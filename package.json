    "@glennsl/bs-jest": "0.5.1",
    "bs-platform": "8.0.3",
    "enzyme-adapter-react-16": "1.15.2",
    "jest": "26.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reason-react": "0.9.1",
    "standard-version": "8.0.2"
  },
